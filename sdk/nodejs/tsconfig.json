{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "config/index.ts",
        "config/vars.ts",
        "filesystem.ts",
        "floatingIp.ts",
        "images.ts",
        "index.ts",
        "instance.ts",
        "instanceStatus.ts",
        "provider.ts",
        "securityGroup.ts",
        "snapshot.ts",
        "sshkey.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts",
        "volume.ts"
    ]
}
